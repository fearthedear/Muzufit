<div class="row">
		<form class = "col s12">
			<div class="row">
				<div class="row col s6">
						<%= form_for(:session, url: login_path) do |f| %>
						<%= f.label :email %>
						<%= f.email_field :email %>
				</div>
			</div>

<div class="row">
<div class="row col s6">
	<%= f.label :password %>
	<%= f.password_field :password %>
	</div>
</div>
	<%= f.submit 'Log in', class: "btn waves-effect waves-light orange darker-1" %>

	</div>
	<% end %>
		</form>
</div>
