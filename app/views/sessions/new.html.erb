    <%= form_for(:session, url: login_path) do |f| %>
     	<div class="row">
		<div class="row col s6">
      <%= f.label :email %>
      <%= f.email_field :email, class: 'form-control' %>

      <%= f.label :password %>
      <%= f.password_field :password, class: 'form-control' %>
  </div>
</div>
      <%= f.submit "Log in", class: "btn btn-primary orange darker-1" %>
    <% end %>
