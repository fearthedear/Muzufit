<!DOCTYPE html>
<html>
<head>
  <title>Muzufit</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
  <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">

</head>



<body <%= sidebarGenerated %> >
  <header <%= sidebarGenerated %> ><%= render 'layouts/header' %></header>
  <main <%= sidebarGenerated %> >

    <%= render :partial => 'layouts/sidenav' if params[:controller] == 'cpostings'%>
    <%= render :partial => 'layouts/sidenav' if params[:controller] == 'subscriptions'%>
    <%= render :partial => 'layouts/sidenav' if params[:controller] == 'users'%>

    <%= yield %>
    <%= debug(params) if Rails.env.development? %>

  </main>
<footer class="page-footer orange darken-1"><%= render 'layouts/footer' %></footer>
</body>
</html>
